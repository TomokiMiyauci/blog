{
  "name": "blog-client",
  "description": "Tech blog for engineer",
  "version": "0.7.1",
  "private": true,
  "license": "MIT",
  "author": "TomokiMiyauci",
  "scripts": {
    "dev": "nuxt-ts",
    "build": "nuxt-ts build",
    "start": "nuxt-ts start",
    "generate": "nuxt-ts generate",
    "lint:markup": "vti diagnostics",
    "lint:script": "eslint --ext .ts,.vue --ignore-path .gitignore .",
    "lint:style": "stylelint **/*.{vue,scss} --ignore-path .gitignore",
    "lint:markdown": "markdownlint src/**/*.md --ignore-path .gitignore",
    "lint": "yarn lint:markup && yarn lint:script && yarn lint:style && yarn lint:markdown",
    "storybook": "nuxt-ts storybook -c storybook.config.ts",
    "build-storybook": "nuxt-ts storybook build -c storybook.config.ts",
    "test:unit": "jest",
    "test:e2e": "cypress run",
    "test:e2e:open": "cypress open",
    "ci:e2e": "server-test 'dev' :3000 'test:e2e'"
  },
  "lint-staged": {
    "*.{ts,vue}": "eslint --fix",
    "*.{scss,vue}": "stylelint --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
