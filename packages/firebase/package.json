{
  "name": "techsrc-functions",
  "private": true,
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "lint": "eslint --ext .ts .",
    "build": "tsc",
    "serve": "yarn build && firebase emulators:start --only functions,hosting",
    "serve:ssr": "yarn build && firebase emulators:start --only functions",
    "shell": "yarn build && firebase functions:shell",
    "start": "yarn shell",
    "deploy": "firebase deploy --only functions",
    "deploy:ssr": "firebase deploy --only functions,hosting",
    "logs": "firebase functions:log",
    "test": "jest --detectOpenHandles"
  },
  "engines": {
    "node": "12"
  },
  "lint-staged": {
    "*.ts": "eslint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "main": "lib/index.js",
  "dependencies": {
    "firebase-admin": "^9.1.0",
    "firebase-functions": "^3.9.1"
  },
  "devDependencies": {
    "@firebase/testing": "^0.20.11",
    "@types/jest": "^26.0.10",
    "@types/node": "^14.6.0",
    "@typescript-eslint/eslint-plugin": "^3.9.1",
    "@typescript-eslint/parser": "^3.9.1",
    "eslint": "^7.7.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "firebase-functions-test": "^0.2.1",
    "husky": "^4.2.5",
    "jest": "^26.4.0",
    "jest-environment-uint8array": "^1.0.0",
    "lint-staged": "^10.2.11",
    "ts-jest": "^26.2.0",
    "typescript": "^3.9.7"
  }
}
