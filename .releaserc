{
  "plugins": [
    ["semantic-release-gitmoji", {
      releaseRules: {
          major: [ ':boom:' ],
          minor: [ ':sparkles:', ':construction_worker:' ],
          patch: [
            ':bug:',
            ':ambulance:',
            ':lock:',
            ':green_heart:',
            ':arrow_up:',
            ':arrow_down:'
          ]
        },
    }],
    "@semantic-release/github",
    ["@semantic-release/npm", {
      "npmPublish": false
    }]
  ]
}
