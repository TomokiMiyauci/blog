{
  "name": "techsrc",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "dev": "nuxt-ts",
    "build": "nuxt-ts build",
    "start": "nuxt-ts start",
    "generate": "nuxt-ts generate",
    "lint:markup": "vti diagnostics",
    "lint:script": "eslint --ext .ts,.vue --ignore-path .gitignore .",
    "lint:style": "stylelint **/*.{vue,scss} --ignore-path .gitignore",
    "lint:markdown": "markdownlint *.md --ignore-path .gitignore",
    "lint": "yarn lint:markup && yarn lint:script && yarn lint:style && yarn lint:markdown",
    "test:unit": "jest",
    "test:e2e": "cypress run",
    "test:e2e:open": "cypress open",
    "ci:e2e": "server-test 'dev' :3000 'test:e2e'"
  },
  "lint-staged": {
    "*.{ts,vue}": "eslint",
    "*.{scss,vue}": "stylelint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
